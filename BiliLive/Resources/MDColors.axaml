<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Add Resources Here -->
    <ResourceDictionary.ThemeDictionaries>
        
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="Surface" Color="#FEF7FF" />
            <SolidColorBrush x:Key="SurfaceVariant" Color="#E7E0EB" />
            <SolidColorBrush x:Key="OnSurface" Color="#1D1B20" />
            <SolidColorBrush x:Key="SurfaceContainer" Color="#F2ECF4" />
            <SolidColorBrush x:Key="SurfaceContainerHigh" Color="#ECE6EE" />
            <SolidColorBrush x:Key="SurfaceContainerHighest" Color="#E7E0E8" />
            <SolidColorBrush x:Key="SecondaryContainer" Color="#E9DEF8" />
            <SolidColorBrush x:Key="Primary" Color="#66558E" />
            <SolidColorBrush x:Key="InversePrimary" Color="#D1BCFD" />
            <SolidColorBrush x:Key="PrimaryContainer" Color="#EADDFF" />
            <SolidColorBrush x:Key="OnPrimary" Color="#FFFFFF" />
        </ResourceDictionary>

        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="Surface" Color="#141218" />
            <SolidColorBrush x:Key="SurfaceVariant" Color="#49454E" />
            <SolidColorBrush x:Key="OnSurface" Color="#E7E0E8" />
            <SolidColorBrush x:Key="SurfaceContainer" Color="#211F24" />
            <SolidColorBrush x:Key="SurfaceContainerHigh" Color="#2B292F" />
            <SolidColorBrush x:Key="SurfaceContainerHighest" Color="#36343A" />
            <SolidColorBrush x:Key="SecondaryContainer" Color="#4A4358" />
            <SolidColorBrush x:Key="Primary" Color="#D1BCFD" />
            <SolidColorBrush x:Key="InversePrimary" Color="#66558E" />
            <SolidColorBrush x:Key="PrimaryContainer" Color="#4E3D75" />    
            <SolidColorBrush x:Key="OnPrimary" Color="#37265C" />
        </ResourceDictionary>
        
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>